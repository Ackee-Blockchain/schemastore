{
  "$schema": "../../schemas/json/docker-bake.json",
  "variable": {
    "TAG": {
      "default": "latest"
    }
  },
  "group": {
    "default": {
      "targets": [
        "webapp-dev"
      ]
    }
  },
  "target": {
    "webapp-dev": {
      "annotations": [
        "com.example.key1=value1",
        "com.example.key2=value2"
      ],
      "attest": [
        "type=sbom"
      ],
      "dockerfile": "Dockerfile.webapp",
      "tags": [
        "docker.io/username/webapp:latest"
      ],
      "args": {
        "buildino": "${increment(123)}"
      }
    }
  },
  "function": {
    "increment": {
      "params": ["number"],
      "result": "${number+1}"
    }
  }
}
